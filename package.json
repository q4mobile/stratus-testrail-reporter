    "@actions/github": "^5.0.x",
    "lodash": "^4.17.x",
    "moment-timezone": "^0.5.x",
    "testrail-api": "^1.3.x",
    "typescript": "^4.5.x"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/node": "^13.13.52",
    "@types/request": "^2.48.8",
    "@vercel/ncc": "^0.29.0",
    "ts-node": "^10.5.0"
  }
